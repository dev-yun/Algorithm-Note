import itertools    # 순열 조합같은 반복되는 데이터를 처리하는 기능 포함
#import random

data = [1, 2, 3]
#for _ in range(10):
#  data.append(random.randint(1,10))

#print(data)

for x in itertools.permutations(data, 2):   # 순열(permutations) 서로다른 data개수에서 2개를 일렬로 정렬
  print(list(x))                            # (수학공식 nPr = n!/ (n-r)!)
  

''' input = [1, 2, 3]
    output = [1,2], [1,3], [2,1], [2,3], [3,1], [3,2]'''
