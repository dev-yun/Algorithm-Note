def rotate_a_matrix_by_90_degree(a):
  n = len(a)      # 행길이 계산
  m = len(a[0])   # 열길이 계산
  result = [[0] * n for _ in range(m)]  # 결과 리스트 초기화
  for i in range(n):
    for j in range(m):
      result[j][n - i - 1] = a[i][j]    # 90도 회전  a(0,0) => result(0,n-1), a(0,n-1) => result(n-1,n-1), a(n-1, n-1) => result(n-1, 0), a(n-1, 0) => result(0,0) 
  return result
